    "version": "0.2.0",
    "configurations": [
        {
            "name": "YCSB-CPP",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/ycsb",
            "console": "integratedTerminal",
            "cwd": "${fileDirname}",
            "args": ["-run", "-db", "lsm2lixdb", "-P", "../workloads/workloada",
             "-s", "-p", "fixedkey8b=true", "-p", "fixedfieldlen=true", "-p",
              "fieldcount=1", "-p", "fieldlength=24", "-p", "threadcount=16"]
          }
    ]
}